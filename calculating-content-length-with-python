<div id="answer-9787720" class="answer accepted-answer" data-answerid="9787720"  itemscope itemtype="http://schema.org/Answer" itemprop="acceptedAnswer">
    <table>
        <tr>
            <td class="votecell">
                

<div class="vote">
        <input type="hidden" name="_id_" value="9787720">
        <a class="vote-up-off" title="This answer is useful">up vote</a>
